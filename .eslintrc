{
    "extends": "eslint:recommended",
    "plugins": [
        "angular"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "jasmine": true,
        "node": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "globals": {
        "module": true,
    },
    "rules": {
        "angular/window-service": 0,
        "angular/document-service": 0,
        "angular/log": 0,
        "no-console": 0,
        "no-useless-escape": 0
    }
}
